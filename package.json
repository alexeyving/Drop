{
  "name": "bp",
  "version": "0.0.1",
  "description": "Boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexeyving/Boilerplate"
  },
  "scripts": {
    "start": "npm-run-all --parallel serve serve:dev build:dev",
    "serve": "node config/server/server.js",
    "serve:dev": "browser-sync start --config config/server/browser-sync.js",
    "build:dev": "webpack --config config/webpack.js"
  },
  "dependencies": {
    "npm-run-all": "3.1.0",
    "express": "4.14.0",
    "express-history-api-fallback": "2.0.0",
    "compression": "1.6.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015-native-modules": "latest",
    "browser-sync": "2.17.5",
    "css-loader": "0.14.5",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^2.0.0",
    "foundation-sites": "^6.3.0",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-plugin": "^2.26.0",
    "jquery": "^3.1.1",
    "node-sass": "^4.3.0",
    "postcss-loader": "^1.2.2",
    "pug": "^2.0.0-beta11",
    "pug-loader": "^2.3.0",
    "sass-loader": "^4.1.1",
    "script-loader": "^0.7.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^1.16.2"
  }
}
